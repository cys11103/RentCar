<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="InsuranceDAO">

	<!-- 보험 조회 -->
	<select id="getInsur" parameterType="int" resultType="insurance">
		SELECT *
		FROM insurance
		WHERE iseq=#{iseq}
	</select>
</mapper>
